<project>
    <actions/>
    <description/>
    <keepDependencies>false</keepDependencies>
    <properties/>
    <scm class="hudson.plugins.scm.koji.KojiSCM" plugin="jenkins-scm-koji-plugin@0.2-SNAPSHOT">
        %{BUILD_PROVIDERS}
        %{XML_RPC_API}
        <downloadDir>rpms</downloadDir>
        <cleanDownloadDir>true</cleanDownloadDir>
        <dirPerNvr>false</dirPerNvr>
        <maxPreviousBuilds>10</maxPreviousBuilds>
    </scm>
    <assignedNode>%{NODES}</assignedNode>
    <canRoam>false</canRoam>
    <disabled>false</disabled>
    <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
    <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
    %{TRIGGER}
    <concurrentBuild>false</concurrentBuild>
    <builders>
    %{BUILDER}
    </builders>
    <publishers>
%{POST_BUILD_TASKS}
%{POST_BUILD_TASK_PLUGIN}
    </publishers>
    <buildWrappers/>
</project>
